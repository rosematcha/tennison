<header class="navbar">
    <a href="/" class="logo">
        <picture>
            <source srcset="/assets/logo.webp" type="image/webp">
            <img src="/assets/logo.png" alt="Site Logo">
        </picture>
    </a>
    <nav>
        <ul>
            {% for page in collections.nav | sort(attribute="data.order") %}
                <li><a id="navbar-member" href="{{ page.url }}">{{ page.data.title }}</a></li>
            {% endfor %}
        </ul>
    </nav>
</header>
